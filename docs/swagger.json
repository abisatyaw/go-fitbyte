    "definitions": {
        "entities.AuthRequest": {
            "type": "object",
            "required": [
                "email",
                "password"
            ],
            "properties": {
                "email": {
                    "type": "string"
                },
                "password": {
                    "type": "string",
                    "maxLength": 32,
                    "minLength": 8
                }
            }
        },
        "entities.Book": {
            "type": "object",
            "properties": {
                "author": {
                    "type": "string"
                },
                "createdAt": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "title": {
                    "type": "string"
                },
                "updatedAt": {
                    "type": "string"
                }
            }
        },
        "entities.DeleteRequest": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                }
            }
        },
        "entities.HeightUnitType": {
            "type": "string",
            "enum": [
                "CM",
                "INCH"
            ],
            "x-enum-varnames": [
                "HeightCM",
                "HeightINCH"
            ]
        },
        "entities.PreferenceType": {
            "type": "string",
            "enum": [
                "CARDIO",
                "WEIGHT"
            ],
            "x-enum-varnames": [
                "PreferenceCardio",
                "PreferenceWeight"
            ]
        },
        "entities.UpdateBookRequest": {
            "type": "object",
            "required": [
                "id"
            ],
            "properties": {
                "author": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "title": {
                    "type": "string"
                }
            }
        },
        "entities.UpdateProfile": {
            "type": "object",
            "required": [
                "height",
                "heightUnit",
                "preference",
                "weight",
                "weightUnit"
            ],
            "properties": {
                "height": {
                    "type": "integer",
                    "maximum": 250,
                    "minimum": 3
                },
                "heightUnit": {
                    "enum": [
                        "CM",
                        "INCH"
                    ],
                    "allOf": [
                        {
                            "$ref": "#/definitions/entities.HeightUnitType"
                        }
                    ]
                },
                "imageUri": {
                    "type": "string",
                    "example": ""
                },
                "name": {
                    "type": "string",
                    "maxLength": 60,
                    "minLength": 2,
                    "example": ""
                },
                "preference": {
                    "enum": [
                        "CARDIO",
                        "WEIGHT"
                    ],
                    "allOf": [
                        {
                            "$ref": "#/definitions/entities.PreferenceType"
                        }
                    ]
                },
                "weight": {
                    "type": "integer",
                    "maximum": 1000,
                    "minimum": 10
                },
                "weightUnit": {
                    "enum": [
                        "KG",
                        "LBS"
                    ],
                    "allOf": [
                        {
                            "$ref": "#/definitions/entities.WeightUnitType"
                        }
                    ]
                }
            }
        },
        "entities.WeightUnitType": {
            "type": "string",
            "enum": [
                "KG",
                "LBS"
            ],
            "x-enum-varnames": [
                "WeightKG",
                "WeightLBS"
            ]
        }
    },
    "securityDefinitions": {
        "BearerAuth": {
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    }
}